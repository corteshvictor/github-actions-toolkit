{
  "name": "github-actions-toolkit",
  "version": "2023.10.20.0",
  "description": "Workflows that use the GitHub App and get, update and delete rules from the protected branch",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev:get": "NAME_FUNC=getBranchProtection node src/local-tests.js",
    "dev:enable": "NAME_FUNC=enableSecurityBranch node src/local-tests.js",
    "dev:disable": "NAME_FUNC=disableSecurityBranch node src/local-tests.js",
    "dev:version": "NAME_FUNC=createVersion node src/local-tests.js",
    "dev:calverVersion": "NAME_FUNC=calverVersion node src/local-tests.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corteshvictor/github-actions-toolkit.git"
  },
  "keywords": [],
  "author": "corteshvictor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corteshvictor/github-actions-toolkit/issues"
  },
  "homepage": "https://github.com/corteshvictor/github-actions-toolkit#readme",
  "dependencies": {
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "calver": "22.9.1",
    "semantic-release": "22.0.5"
  },
  "engines": {
    "node": "18"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "devDependencies": {
    "dotenv": "16.3.1"
  }
}